#!/usr/bin/env node

var pkj        = require(__dirname + '/../package.json'),
    wp         = require(__dirname + '/../'),
    _          = require('underscore'),
    OptCollect = require('optcollect');

var opts = new OptCollect();

var placeName    = _.last(opts.args);
var option = _.first(opts.options);

wp.loadBookmarkFile(__dirname + '/../var/bookmark.json');

var browser = wp.Browser.find(option);

var place = wp.Place.find(placeName) || new wp.Place(placeName);

place.open(browser, function (err, res) {
	if (err) {
		console.error(err);
		return;
	}
});